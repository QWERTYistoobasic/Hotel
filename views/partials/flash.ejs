<% if(success && success.length) {%>
    <div class="alert alert-success alert-dismissible  fade show" role="alert" id="myFlash">
        <%= success %>
        <button type="button" class="btn-close" aria-label="Close"  data-bs-dismiss="modal"></button>
    </div>
    <% } %>
    <% if(error && error.length) {%>
        <div class="alert alert-danger alert-dismissible fade show" role="alert" id="myFlash">
            <%= error %>
            <button type="button" class="btn-close" aria-label="Close" data-bs-dismiss="modal"></button> 
        </div>
    <% } %>